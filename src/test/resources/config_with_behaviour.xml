<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE config PUBLIC
        "-//json/json-ignore mapping DTD 1.0//EN"
        "https://rkonovalov.github.io/json-ignore-schema-1.1.dtd">
<config>
    <controller class-name="com.jfilter.mock.webservice.WSMethod">
        <strategy attribute-name="ROLE" attribute-value="USER">
            <filter class="com.jfilter.mock.MockUser" behaviour="KEEP_FIELDS">
                <field name="id"/>
                <field name="email"/>
            </filter>
        </strategy>
    </controller>
</config>