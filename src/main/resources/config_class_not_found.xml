<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE config PUBLIC
        "-//json/json-ignore mapping DTD 1.0//EN"
        "https://rkonovalov.github.io/json-ignore-schema-1.0.dtd">
<!--suppress RequiredAttributes -->
<config>
    <controller class-name="com.json.ignore.mock.MockMethods">
        <strategy attribute-name="ROLE" attribute-value="ADMIN">

            <filter class="com.json.ignore.mock.ClassNotFound">
                <field name="id"/>
                <field name="password"/>
            </filter>

            <filter class="">
                <field name="id"/>
                <field name="password"/>
            </filter>

            <filter >
                <field name="id"/>
                <field name="password"/>
            </filter>
        </strategy>
    </controller>
</config>